{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:8443",
        "Certificate": {
          "Path": "/etc/ssl/certs/apache-selfsigned.crt",
          "KeyPath": "/etc/ssl/private/apache-selfsigned.key"
        }
      }
    }
  }
}
